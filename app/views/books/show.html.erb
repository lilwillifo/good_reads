<header class='high'>
  Highest Rating: <%= @book.highest_rating %>
  <%= @book.highest_review.body %>
  <%= @book.highest_review.user.name %>
</header>

<header class='low'>
  Lowest Rating: <%= @book.lowest_rating %>
</header>

<%= @book.title %>
<% @book.reviews.each do |review| %>
  <%= review.body %>
  <%= review.rating %>
  <%= review.user.name %>
<% end %>
Average Rating: <%= @book.average_rating %>
